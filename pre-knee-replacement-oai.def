Bootstrap: docker
From: mambaorg/micromamba

%files
   environment.yml

%post
    CONDA_OVERRIDE_CUDA=12.0 micromamba create -q -y -f environment.yml -p /home/home02/scjb/pred-knee-replacement-oai
    micromamba clean -aqy
    micromamba config set --system use_lockfiles false

%runscript
    micromamba run -p /home/home02/scjb/pred-knee-replacement-oai "$@"